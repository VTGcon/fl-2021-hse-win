{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "VYACHESLAVLANGUAGE13371488",
	"patterns": [
		{
			"include": "#number"
		},
		{
			"include": "#type_of_vertex"
		},
		{
			"include": "#weights"
		}
	],
	"repository": {
		"number": {
			"patterns": [
				{
					"name": "constant.character.escape.vyacheslav",
					"match": "\\K[^!]*!"
				}
			]
		},
		"type_of_vertex": {
			"patterns": [
				{
					"name": "keyword.control.n",
					"match": "\\K[^@]*@"
				}
			]
		},
		"weights": {
			"patterns": [
				{
					"name": "string.quoted.double.n",
					"match": "[\\d],[\\d]+,\\K[^;]*;"
				}
			]
		}
	},
	"scopeName": "source.vyacheslav"
}
